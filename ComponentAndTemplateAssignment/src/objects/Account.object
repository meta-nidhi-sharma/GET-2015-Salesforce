    <validationRules>
        <fullName>AccountNumberValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT( ISBLANK(AccountNumber)),NOT(ISNUMBER(AccountNumber)) )</errorConditionFormula>
        <errorMessage>Account number must be a number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>BillingPostalCodeValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(BillingCountry = &quot;USA&quot;, BillingCountry = &quot;US&quot;),
NOT(REGEX(BillingPostalCode, &quot;\\d{5}(-\\d{4})?&quot;))
)</errorConditionFormula>
        <errorMessage>Billing postal code must be in format 99999 OR 99999-9999</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CreditCardNumberValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(CreditCardNumber__c )),NOT( REGEX(  CreditCardNumber__c ,  
&quot;(((\\d{4}-){3}\\d{4})|\\d{16})?&quot;)))</errorConditionFormula>
        <errorDisplayField>CreditCardNumber__c</errorDisplayField>
        <errorMessage>Credit Card Number must be in this format: 9999-9999-9999-9999 or 9999999999999999</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DateVaidationRule</fullName>
        <active>true</active>
        <errorConditionFormula>CASE( MOD( date__c - DATE(1900, 1, 7), 7), 0, 0, 6,0,1) = 0</errorConditionFormula>
        <errorDisplayField>date__c</errorDisplayField>
        <errorMessage>Invalid date (You have entered a date which wasn&apos;t a working day)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MaxPCTRule</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(AND( MaxPCT__c &lt;=100,MaxPCT__c &gt;=0))</errorConditionFormula>
        <errorMessage>MaxPCT can be between 0-100%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MultipleOfFiveRule</fullName>
        <active>true</active>
        <errorConditionFormula>MOD(Multipleof5__c,5) &lt;&gt; 0</errorConditionFormula>
        <errorMessage>Number must be a multiple of 5</errorMessage>
    </validationRules>
